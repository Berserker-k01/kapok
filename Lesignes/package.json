{
  "name": "lesigne-monorepo",
  "version": "1.0.0",
  "description": "Plateforme e-commerce multi-tenant Lesigne",
  "private": true,
  "scripts": {
    "install:all": "npm run install:server && npm run install:user-panel && npm run install:admin-panel",
    "install:server": "cd server && npm install",
    "install:user-panel": "cd user-panel && npm install", 
    "install:admin-panel": "cd admin-panel && npm install",
    "install:client": "cd client && npm install",
    "dev:server": "cd server && npm run dev",
    "dev:user-panel": "cd user-panel && npm run dev",
    "dev:admin-panel": "cd admin-panel && npm run dev",
    "dev:client": "cd client && npm run dev",
    "start:server": "cd server && npm start",
    "start:user-panel": "cd user-panel && npm run build && npm run preview",
    "start:admin-panel": "cd admin-panel && npm run build && npm run preview",
    "build:all": "npm run build:user-panel && npm run build:admin-panel",
    "build:user-panel": "cd user-panel && npm run build",
    "build:admin-panel": "cd admin-panel && npm run build",
    "build:client": "cd client && npm run build",
    "test": "echo \"Tests Ã  implÃ©menter\" && exit 0"
  },
  "keywords": [
    "ecommerce",
    "multi-tenant",
    "saas",
    "react",
    "nodejs",
    "postgresql"
  ],
  "author": "Lesigne Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
